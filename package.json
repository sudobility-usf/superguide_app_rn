  "displayName": "Starter App",
  "private": true,
  "scripts": {
    "prestart": "node scripts/merge-env.js",
    "start": "ENVFILE=.env.merged react-native start --port 8084",
    "preandroid": "node scripts/merge-env.js",
    "android": "ENVFILE=.env.merged react-native run-android --port 8084",
    "preios": "node scripts/merge-env.js",
